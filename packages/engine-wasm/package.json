{
  "name": "@orpheus/engine-wasm",
  "version": "0.1.0",
  "description": "WebAssembly driver for Orpheus SDK - browser-side audio engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "default": "./dist/worker.js"
    }
  },
  "files": [
    "dist",
    "build/*.wasm",
    "build/*.js",
    "build/integrity.json"
  ],
  "scripts": {
    "build:wasm": "./scripts/build-wasm.sh",
    "build:ts": "tsc",
    "build": "pnpm run build:wasm && pnpm run build:ts",
    "clean": "rm -rf dist build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@orpheus/contract": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": [
    "orpheus",
    "audio",
    "wasm",
    "webassembly",
    "driver"
  ],
  "author": "Orpheus SDK Contributors",
  "license": "MIT"
}
