{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://orpheus.sdk/schemas/v1.0.0-beta/events/RenderProgress.json",
  "title": "RenderProgress",
  "description": "Percentage updates for renders (â‰¤10 Hz)",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "RenderProgress"
    },
    "timestamp": {
      "type": "number",
      "description": "Event timestamp (milliseconds since epoch)"
    },
    "percentage": {
      "type": "number",
      "description": "Render completion percentage (0-100)",
      "minimum": 0,
      "maximum": 100
    },
    "renderId": {
      "type": "string",
      "description": "Render operation identifier"
    },
    "sequenceId": {
      "type": "number",
      "description": "Monotonic sequence ID for ordering"
    }
  },
  "required": ["type", "timestamp", "percentage"],
  "additionalProperties": false
}
