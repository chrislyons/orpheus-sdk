    "typescript": "^5.3.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "gypfile": false,
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "20.0.0",
    "arch": "x64"
  },
  "keywords": [
    "orpheus",
    "audio",
    "napi",
    "native",
    "addon"
  ],
  "license": "MIT"
