{
  "name": "@orpheus/client",
  "version": "0.1.0-alpha.0",
  "private": false,
  "description": "Orpheus Client - Unified driver broker for Orpheus SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js"
  },
  "dependencies": {
    "@orpheus/contract": "workspace:*"
  },
  "peerDependencies": {
    "@orpheus/engine-service": "workspace:*",
    "@orpheus/engine-native": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@orpheus/engine-service": {
      "optional": true
    },
    "@orpheus/engine-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "orpheus",
    "audio",
    "client",
    "broker",
    "driver"
  ],
  "license": "MIT"
}
