{
  "packageManager": "pnpm@8.15.4",
  "name": "orpheus-sdk",
  "private": true,
  "version": "0.0.0",
  "description": "C++ SDK for professional audio applications.",
  "scripts": {
    "prepare": "husky",
    "test": "./tests/integration/run-tests.sh",
    "lint": "pnpm run lint:cpp",
    "lint:cpp": "scripts/lint-cpp.sh",
    "format": "pnpm run format:cpp",
    "format:cpp": "find src include -name '*.cpp' -o -name '*.h' -o -name '*.hpp' | xargs clang-format -i"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "madge": "^8.0.0",
    "prettier": "^3.3.2"
  },
  "pnpm": {
    "overrides": {
      "wait-on": "8.0.4",
      "camera-controls": "2.8.3",
      "@vercel/oidc": "2.0.2",
      "@orama/orama": "3.1.0",
      "lru-cache": "10.4.3",
      "semver": "7.6.3",
      "rimraf": "5.0.5",
      "minimatch": "9.0.5",
      "commander": "11.1.0",
      "marked": "12.0.2",
      "cross-spawn": "7.0.6"
    },
    "patchedDependencies": {
      "minimatch@9.0.5": "patches/minimatch@9.0.5.patch"
    }
  }
}
